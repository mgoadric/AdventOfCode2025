








with open("data/1/input.txt") as f:
    data = f.readlines()
    dial = 50
    zeros = 0
    for rot in data:
        if rot[0] == "R":
            dial += int(rot[1:])
        else:
            dial -= int(rot[1:])
        dial %= 100
        if dial == 0:
            zeros += 1
    print(zeros)








with open("data/1/testing3.txt") as f:
    data = f.readlines()
    dial = 50
    zeros = 0
    for rot in data:
        rot = rot.strip()
        print(rot)
        direction = rot[0]
        count = int(rot[1:])
        if direction == "R":
            dial += count
            print(dial, dial // 100)
            zeros += dial // 100
        else:
            dial -= count
            print(dial, dial // 100)
            zeros -= dial // 100
        dial %= 100
    print(zeros)



